<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="2008">
    <defaultcontrol always="true">450</defaultcontrol>
    <allowoverlay>yes</allowoverlay>

    <controls>

        <control type="group">
            <animation effect="slide" start="0,700" end="0" time="200">WindowOpen</animation>
            <include>Animation.FadeOut</include>
            
            <control type="image">
                <posy>210</posy>
                <width>1920</width>
                <height>764</height>
                <texture border="5" colordiffuse="TopBar">common/white.png</texture>
            </control>

            <control type="label">
                <description>Header Label</description>
                <textcolor>TopBarIcon</textcolor>
                <include>DefSettingsHeader</include>
                <info>ListItem.Label</info>
            </control>

            <control type="list" id="450">
                <posx>525</posx>
                <posy>117</posy>
                <width>870</width>
                <height>552</height>
                <onleft>450</onleft>
                <onright>450</onright>
                <onup>450</onup>
                <ondown>450</ondown>
                <itemlayout height="69">


                    <control type="label">
                        <posx>30</posx>
                        <width>810</width>
                        <info>ListItem.Label</info>
                        <textcolor>TopBarIcon</textcolor>
                    </control>

                </itemlayout>

                <focusedlayout height="69">


                    <control type="label">
                        <posx>30</posx>
                        <width>810</width>
                        <info>ListItem.Label</info>
                        <textcolor>Highlight</textcolor>
                    </control>

                </focusedlayout>

            </control>

        </control>

    </controls>

</window>
