<?xml version="1.0" encoding="UTF-8"?>
<!-- Foundation -->
<window id="122">
    <defaultcontrol always="true">2</defaultcontrol>
    <allowoverlay>yes</allowoverlay>

    <controls>

        <control type="group">
            <animation effect="slide" start="0,700" end="0" time="200">WindowOpen</animation>
            <include>Animation.FadeOut</include>
            
            <control type="image">
                <posy>210</posy>
                <width>1920</width>
                <height>764</height>
                <texture border="5" colordiffuse="TopBar">common/white.png</texture>
            </control>
            
            <control type="label" id="20">
                <description>Header Label</description>
                <textcolor>TopBarIcon</textcolor>
                <include>DefSettingsHeader</include>
                <label>31032</label>
                
            </control>

            <control type="list" id="2">
                <description>Preset List</description>
                <posx>15</posx>
                <posy>87</posy>
                <width>870</width>
                <height>552</height>
                <onleft>noop</onleft>
                <onright>noop</onright>
                <onup>2</onup>
                <ondown>2</ondown>

                <itemlayout height="69">

                    <control type="label">
                        <posx>30</posx>
                        <width>810</width>
                        <info>ListItem.Label</info>
                        <textcolor>TopBarIcon</textcolor>
                    </control>

                </itemlayout>

                <focusedlayout height="69">

                    <control type="label">
                        <posx>30</posx>
                        <width>810</width>
                        <info>ListItem.Label</info>
                        <textcolor>Highlight</textcolor>
                    </control>

                </focusedlayout>

            </control>

        </control>

    </controls>

</window>
