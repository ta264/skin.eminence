<?xml version="1.0" encoding="UTF-8"?>
<addon
	id="skin.eminence"
	version="0.9.40"
	name="Eminence"
	provider-name="jurialmunkey">
	<requires>
		<import addon="xbmc.gui" version="5.0.0"/>
		<import addon="service.skin.widgets" version="0.0.25"/>
		<import addon="script.globalsearch" version="1.1.1"/>
		<import addon="script.artistslideshow" version="1.5.0"/>
		<import addon="script.tvtunes" version="3.0.6"/>
		<import addon="service.mythtv.powersave" version="0.1.0"/>
	</requires>
	<extension
    point="xbmc.gui.skin"
    defaultthemename="textures.xbt"
    debugging="false"
    effectslowdown="1">
